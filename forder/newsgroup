   "source": [
    "%%time \n",
    "embeddings_index = {}\n",
    "\n",
    "f = open('./dataset/glove.6B.100d.txt')\n",
    "for line in tqdm(f):\n",
    "    values = line.split(' ')\n",
    "    word = values[0] \n",
    "    coefs = np.asarray(values[1:], dtype='float32')\n",
    "    embeddings_index[word] = coefs\n",
    "f.close()\n",
    "\n",
    "print(\"Words vectors:\", len(embeddings_index))"
   ]
  },
